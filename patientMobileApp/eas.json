{
  "cli": {
    "version": ">= 3.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_SUPABASE_URL": "your_supabase_url_here",
        "EXPO_PUBLIC_SUPABASE_ANON_KEY": "your_supabase_anon_key_here"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_SUPABASE_URL": "your_supabase_url_here",
        "EXPO_PUBLIC_SUPABASE_ANON_KEY": "your_supabase_anon_key_here"
      }
    },
    "production": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_SUPABASE_URL": "your_supabase_url_here",
        "EXPO_PUBLIC_SUPABASE_ANON_KEY": "your_supabase_anon_key_here"
      }
    }
  },
  "submit": {
    "production": {}
  }
} 